{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Type Catalog",
    "type": "array",
    "items": {
        "title": "type's catalog",
        "type": "object",
        "properties": {
        	"typename": {
				"type":"string"
			},
            "pids": {
				"type":"array",
				"description": "pid set",
				"items":
					{
						"type":"number",
						"minimum": 1000,
						"maximum": 9999
					}
			}
        },
        "required": ["typename", "pids"]
    }
}