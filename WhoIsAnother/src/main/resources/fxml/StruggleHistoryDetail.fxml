<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>

<ScrollPane fx:id="rootLayout" fitToWidth="true" prefHeight="400.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.wia.controller.StruggleHistoryDetailController">
   <content>
      <GridPane fx:id="gridPane" alignment="CENTER" styleClass="layout">
        <!-- TODO Add Nodes -->
        <children>
          <BarChart fx:id="barChart" GridPane.columnIndex="0" GridPane.rowIndex="1">
            <xAxis>
              <CategoryAxis side="BOTTOM" />
            </xAxis>
            <yAxis>
              <NumberAxis side="LEFT" />
            </yAxis>
          </BarChart>
          <PieChart id="PieChart" fx:id="pieChart" GridPane.columnIndex="0" GridPane.rowIndex="2" />
            <HBox fx:id="hBox" maxHeight="30.0" minHeight="30.0" />
        </children>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="-1.0" minWidth="10.0" prefWidth="685.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" percentHeight="4.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="-1.0" minHeight="10.0" percentHeight="48.0" prefHeight="-1.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="-1.0" minHeight="10.0" percentHeight="48.0" prefHeight="-1.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <stylesheets>
          <URL value="@../css/general.css" />
        </stylesheets>
      </GridPane>
   </content>
</ScrollPane>
