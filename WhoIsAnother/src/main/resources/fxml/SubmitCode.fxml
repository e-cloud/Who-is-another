<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="com.wia.controller.SubmitController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints maxHeight="40.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
  <children>
    <TextArea fx:id="code" layoutX="65.0" layoutY="81.0" prefHeight="471.0" prefWidth="800.0"
      GridPane.rowIndex="1">
      <GridPane.margin>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </GridPane.margin>
    </TextArea>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
      <children>
        <Button fx:id="backButton" mnemonicParsing="false" prefHeight="23.0" prefWidth="66.0"
          text="后退" />
        <Label text="language：" />
        <ComboBox fx:id="language" prefHeight="23.0" prefWidth="107.0">
          <items>
            <FXCollections fx:factory="observableArrayList">
              <String fx:value="G++" />
              <String fx:value="GCC" />
              <String fx:value="C++" />
              <String fx:value="C" />
              <String fx:value="Pascal" />
              <String fx:value="Java" />
            </FXCollections>
          </items>
        </ComboBox>
        <Label text="用户名：" />
        <TextField fx:id="username" />
        <Label text="密码：" />
        <PasswordField fx:id="password" />
        <Button fx:id="submitButton" mnemonicParsing="false" text="提交" />
      </children>
    </HBox>
  </children>
</GridPane>
