<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>

<GridPane fx:id="rootLayout" maxHeight="600.0" maxWidth="800.0" prefHeight="600.0" prefWidth="800.0" styleClass="layout" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.wia.controller.StruggleHistoryMonthController">
  <children>
    <HBox id="HBox" alignment="CENTER_LEFT" GridPane.columnIndex="0" GridPane.rowIndex="0">
      <children>
        <Hyperlink fx:id="generalLink" styleClass="Hyperlink" text="概览" />
        <Label text="&gt;" />
        <Hyperlink fx:id="yearLink" styleClass="Hyperlink" text="2014年" />
        <Label text="&gt;" />
        <Hyperlink fx:id="monthLink" styleClass="Hyperlink" text="1月" />
      </children>
      <GridPane.margin>
        <Insets left="10.0" fx:id="x1" />
      </GridPane.margin>
    </HBox>
    <Label fx:id="barlabel" styleClass="text-title" text="提交题目数2014年1月各日分布" GridPane.columnIndex="0" GridPane.margin="$x1" GridPane.rowIndex="1" />
    <Label fx:id="pielabel" styleClass="text-title" text="2014年1月做题情况总体分析" GridPane.columnIndex="0" GridPane.margin="$x1" GridPane.rowIndex="3" />
    <PieChart id="PieChart" fx:id="monthPieChart" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="4" />
      <BarChart fx:id="monthBarChart" GridPane.rowIndex="2">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </BarChart>
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="40.0" percentHeight="8.0" prefHeight="-1.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="30.0" percentHeight="8.0" prefHeight="-1.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" percentHeight="45.0" prefHeight="-1.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="30.0" percentHeight="8.0" prefHeight="-1.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" percentHeight="45.0" prefHeight="-1.0" vgrow="SOMETIMES" />
  </rowConstraints>
  <stylesheets>
    <URL value="@../css/general.css" />
  </stylesheets>
</GridPane>
